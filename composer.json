{
  "name": "ocallit/tabulator",
  "description": "Util functions for tabulator",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ocallit/sqler": "*",
    "slowprog/composer-copy-file": "~0.3",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "preferred-install": {
    "ocallit/sqler": "source"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ocallit\\Tabulator\\": "src/inc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ocallit\\Tabulator\\Tests\\": "tests/"
    }
  }
}
